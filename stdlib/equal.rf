(def equal a b
	(if (len &args)
		(raise "equal: invalid arguments")
	)
	(if (== (len &a) (len &b))
		(null (set i 0)
		      (set res 1)
		)
		(while (< &i (len &a))
			(if (!= (index &i &a) (index &i &b/))
				(null (set res 0)
				      (set i (len &a))
				)
			)
		)
		(ret &ret)
	else
		(ret 0)
	)
)
