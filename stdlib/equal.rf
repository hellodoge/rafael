(def equal arg
    (if (== (len &arg) (len &args))
        (null (set i 0)
              (set res 1)
        )
        (while (< &i (len &arg))
            (if (!= (index &i &arg) (index &i &args))
                (null (set res 0)
                      (set i (len &arg))
                )
            )
        )
        (ret &res)
    else
        (ret 0)
    )
)
